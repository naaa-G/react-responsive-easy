{
  "name": "@react-responsive-easy/figma-plugin",
  "version": "1.0.0",
  "description": "Figma plugin for React Responsive Easy design tokens and responsive preview",
  "main": "dist/code.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "figma",
    "plugin",
    "react",
    "responsive",
    "design-tokens",
    "design-system"
  ],
  "author": "naa-G",
  "license": "MIT",
  "dependencies": {
    "@react-responsive-easy/core": "workspace:*"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.98.0",
    "@types/node": "^20.10.6",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "figma": {
    "id": "react-responsive-easy",
    "name": "React Responsive Easy",
    "main": "dist/code.js",
    "ui": "dist/ui.html",
    "capabilities": [
      "inspect"
    ],
    "enableProposedApi": false,
    "editorType": [
      "figma",
      "figjam"
    ],
    "networkAccess": {
      "allowedDomains": [
        "https://api.react-responsive-easy.dev"
      ]
    }
  }
}
