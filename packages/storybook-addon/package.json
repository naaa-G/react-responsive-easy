{
  "name": "@yaseratiar/react-responsive-easy-storybook-addon",
  "version": "1.0.2",
  "description": "Storybook addon for React Responsive Easy component documentation and testing",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "preset.js",
    "register.js",
    "manager.js",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "storybook",
    "addon",
    "react",
    "responsive",
    "design-system",
    "documentation"
  ],
  "author": "naa-G",
  "license": "MIT",
  "dependencies": {
    "@yaseratiar/react-responsive-easy-core": "workspace:*",
    "@yaseratiar/react-responsive-easy-performance-dashboard": "workspace:*",
    "@storybook/addons": "^7.6.6",
    "@storybook/api": "^7.6.6",
    "@storybook/components": "^7.6.6",
    "@storybook/core-events": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/theming": "^7.6.6",
    "@storybook/types": "^7.6.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "rollup": "^4.9.4",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.1",
    "@vitest/coverage-v8": "^1.1.1"
  },
  "peerDependencies": {
    "@storybook/addons": "^7.0.0",
    "@storybook/api": "^7.0.0",
    "@storybook/components": "^7.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "storybook": {
    "displayName": "React Responsive Easy",
    "supportedFrameworks": [
      "react",
      "react-vite",
      "nextjs"
    ],
    "icon": "https://cdn.react-responsive-easy.dev/icon.svg"
  }
}
