{
  "name": "@react-responsive-easy/vite-plugin",
  "version": "1.0.0",
  "description": "Vite plugin for React Responsive Easy integration",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "dev": "tsc --noEmit --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rimraf dist"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "responsive",
    "react",
    "build-time",
    "optimization",
    "css",
    "babel"
  ],
  "author": "naa-G",
  "license": "MIT",
  "dependencies": {
    "@react-responsive-easy/babel-plugin": "workspace:*",
    "@react-responsive-easy/postcss-plugin": "workspace:*",
    "@react-responsive-easy/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naaa-G/react-responsive-easy.git",
    "directory": "packages/vite-plugin"
  },
  "bugs": {
    "url": "https://github.com/naaa-G/react-responsive-easy/issues"
  },
  "homepage": "https://github.com/naaa-G/react-responsive-easy#readme"
