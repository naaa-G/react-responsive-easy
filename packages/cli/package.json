  "scripts": {
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "build:watch": "rollup -c --watch",
    "build:clean": "rimraf dist && pnpm build",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit --strict",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:report": "eslint src --ext .ts,.tsx --format json --output-file lint-report.json",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=json --outputFile=test-results.json",
    "clean": "rimraf dist coverage test-results.json lint-report.json",
    "prebuild": "pnpm clean && pnpm type-check",
    "prepublishOnly": "pnpm build:prod && pnpm test:ci",
    "validate": "pnpm type-check && pnpm lint && pnpm test",
    "validate:ci": "pnpm type-check:strict && pnpm lint && pnpm test:ci"
  },
