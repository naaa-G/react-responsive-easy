  "name": "@yaseratiar/react-responsive-easy-cli",
  "version": "2.0.0",
  "type": "module",
  "description": "ðŸš€ Enterprise-grade CLI tools with AI integration and performance monitoring for React Responsive Easy",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "require": "./dist/cli.js"
    }
  },
  "bin": {
    "rre": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "responsive",
    "cli",
    "enterprise",
    "ai",
    "machine-learning",
    "performance",
    "monitoring",
    "analytics",
    "optimization",
